#include<stdio.h>
#include<string.h>
main() 
{
char a[100],b[100];
int l, i, c=0;
printf (“enter username=”) ;
scanf(“%s”,a);
printf (“re enter username=”) ;
scanf(“%s”,b);
l=strlen (a) ;
for(i=0;i<l;i++) 
{
if(a[i]==b[i]) ;
{
c++;
}
}
if(l==c) 
{
printf(“valid”) ;
}
else
{
printf(“invalid”) ;
}
return 0;
}




